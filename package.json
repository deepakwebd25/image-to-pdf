{
  "name": "image-to-pdf",
  "version": "1.0.0",
  "private": true,

  "main": "main.js",
  "homepage": "./",

  "description": "Image to PDF Converter",
  "author": "Deepak",

  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron": "electron .",
    "dist": "electron-builder --config"
  },

  "build": {
    "extends": null,

    "appId": "com.deepak.imagetopdf",
    "productName": "Image To PDF",

    "directories": {
      "app": "."
    },

    "files": [
      "build/**/*",
      "main.js",
      "package.json"
    ],

    "extraMetadata": {
      "main": "main.js"
    },

    "win": {
      "target": "nsis"
    }
  }
}
